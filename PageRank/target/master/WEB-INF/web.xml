<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
 <web-app>
     <display-name>PageRank (Master)</display-name>
     
     <servlet>
         <servlet-name>MasterServlet</servlet-name>
         <servlet-class>edu.upenn.cis455.mapreduce.master.MasterServlet</servlet-class>
         <init-param>
           <param-name>storagedir</param-name>
           <param-value>pagerank/storage/master</param-value>
         </init-param>
     </servlet>

     <servlet-mapping>
         <servlet-name>MasterServlet</servlet-name>
         <url-pattern>/*</url-pattern>
     </servlet-mapping>
</web-app>
